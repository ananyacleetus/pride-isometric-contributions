{
  "name": "GitHub Pride Isometric Contributions",
  "version": "1.0.0",
  "description": "Renders an isometric pixel view of GitHub contribution graphs in PRIDE rainbow colors.",
  "content_scripts": [ {
    "css": [ "iso.css" ],
    "js": [ "jquery.min.js", "obelisk.min.js", "iso.js" ],
    "matches": [ "http://*/*", "https://*/*" ]
  }],
  "permissions": [
    "storage"
  ],
  "icons" : {
    "16" : "icon-16.png",
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "manifest_version": 1
}
